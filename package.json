{
  "name": "pinky",
  "version": "0.1.0",
  "description": "Simple functional canvas library",
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -q -d dist",
    "coverage": "nyc ava",
    "demo": "webpack --config examples/webpack.config.js && http-server examples",
    "prepublish": "npm run build",
    "test": "ava",
    "watch": "babel src -w -d dist"
  },
  "keywords": [
    "canvas"
  ],
  "author": "Keith Bingman",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.17.0",
    "babel-cli": "^6.9.0",
    "babel-core": "^6.25.0",
    "babel-loader": "6.2.9",
    "babel-preset-env": "^1.6.0",
    "http-server": "^0.10.0",
    "jsdom": "^9.2.1",
    "nyc": "^10.0.0",
    "sinon": "^1.17.4",
    "webpack": "^3.3.0"
  },
  "ava": {
    "require": ["babel-core/register", "./test/helpers/setup-browser-env.js"],
    "babel": "inherit",
    "concurrency": 2
  }
}
